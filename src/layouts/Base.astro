---
import '../global.css'
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="es">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="shortcut icon" href="/Cinnamon-Overoll.ico" type="image/x-icon">
		<title>{title}</title>
	</head>

	<body><slot /></body>

</html>
